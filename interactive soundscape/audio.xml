    <ObjectBasedAudio gain="0dB" class="shootout"  positionX="3" positionZ="3" src="audio/shootout.mp3" loop="false"/>
    <ObjectBasedAudio gain="0dB" class="outro"  positionX="-4" positionZ="-2" src="audio/outro.mp3" loop="false"/>

    
</Audio>
